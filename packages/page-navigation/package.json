{
    "name": "@max_alieksieiev/react-pdf-viewer-page-navigation",
    "version": "0.0.1",
    "description": "A React component to view a PDF document",
    "license": "MIT",
    "author": {
        "name": "Maks Alieksieiev",
        "email": "corp.workMA@gmail.com"
    },
    "keywords": [
        "pdf viewer"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/MaxAlieksieiev/react-pdf-viewer",
        "directory": "packages/page-navigation"
    },
    "bugs": {
        "url": "https://github.com/MaxAlieksieiev/react-pdf-viewer/issus"
    },
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib/cjs/*.*",
        "lib/styles/*.*",
        "lib/index.d.ts",
        "lib/index.js"
    ],
    "dependencies": {
        "@max_alieksieiev/react-pdf-viewer-core": "0.0.4"
    },
    "peerDependencies": {
        "react": ">=16.8.0",
        "react-dom": ">=16.8.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "clean": "rm -rf lib && mkdir lib",
        "build": "npm run clean && npm run js && npm run css",
        "js": "cp src/index.d.ts lib && cp dist/index.js lib && rollup -c ../../rollup.config.cjs",
        "css": "sass --no-source-map --style compressed src/styles/index.scss lib/styles/index.css",
        "release": "npm publish --access public"
    }
}
